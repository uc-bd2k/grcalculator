t-style-type:none}.lst-kix_qkescwycupop-4>li:before{content:"\0025cb  "}ul.lst-kix_4v4tuo10gdxg-2{list-style-type:none}.lst-kix_c6mggzy9bkhi-1>li:before{content:"" counter(lst-ctn-kix_c6mggzy9bkhi-1,lower-latin) ". "}ul.lst-kix_4v4tuo10gdxg-3{list-style-type:none}.lst-kix_9dbpnvhyro1-7>li:before{content:"\0025cb  "}.lst-kix_lx3h5q6cjds-6>li{counter-increment:lst-ctn-kix_lx3h5q6cjds-6}.lst-kix_lx3h5q6cjds-6>li:before{content:"" counter(lst-ctn-kix_lx3h5q6cjds-6,decimal) ". "}.lst-kix_4vw5dpxrimee-0>li:before{content:"\0025cf  "}.lst-kix_c6mggzy9bkhi-3>li{counter-increment:lst-ctn-kix_c6mggzy9bkhi-3}.lst-kix_ixtk3xu5n7qn-0>li{counter-increment:lst-ctn-kix_ixtk3xu5n7qn-0}.lst-kix_lx3h5q6cjds-4>li:before{content:"" counter(lst-ctn-kix_lx3h5q6cjds-4,lower-latin) ". "}.lst-kix_qkescwycupop-0>li:before{content:"\0025cf  "}.lst-kix_ixtk3xu5n7qn-6>li{counter-increment:lst-ctn-kix_ixtk3xu5n7qn-6}.lst-kix_lx3h5q6cjds-0>li:before{content:"\0025cf  "}.lst-kix_lx3h5q6cjds-2>li:before{content:"" counter(lst-ctn-kix_lx3h5q6cjds-2,lower-roman) ". "}.lst-kix_4v4tuo10gdxg-8>li:before{content:"\0025a0  "}.lst-kix_4v4tuo10gdxg-6>li:before{content:"\0025cf  "}.lst-kix_4v4tuo10gdxg-2>li:before{content:"\0025a0  "}.lst-kix_4v4tuo10gdxg-4>li:before{content:"\0025cb  "}.lst-kix_4v4tuo10gdxg-0>li:before{content:"\0025cf  "}.lst-kix_c6mggzy9bkhi-5>li:before{content:"" counter(lst-ctn-kix_c6mggzy9bkhi-5,lower-roman) ". "}ol.lst-kix_lx3h5q6cjds-6.start{counter-reset:lst-ctn-kix_lx3h5q6cjds-6 0}ol{margin:0;padding:0}      table td,table th{padding:0}.c4{background-color:#ffffff;color:#333333;vertical-align:sub;font-size:9pt;font-family:"Times New Roman"}.c2{margin-left:36pt;orphans:2;widows:2;padding-left:0pt}      .c6{orphans:2;widows:2;height:11pt}      .c9{background-color:#ffffff;font-size:9pt;color:#333333}      .c24{background-color:#ffffff;max-width:511.2pt;padding:36pt 50.4pt 36pt 50.4pt}      .c19{color:#1155cc;text-decoration:underline}      .c7{orphans:2;widows:2}      .c8{font-